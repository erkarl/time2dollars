{
  "name": "time2dollars",
  "version": "1.0.0",
  "description": "Time to Dollars Calculator",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --hot --config webpack.config.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS",
    "test:server": "./node_modules/.bin/karma start",
    "build": "npm run clean && npm run build:production",
    "build:production": "webpack --progress --colors --config webpack.production.config.js",
    "clean": "rm -Rf build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erkarl/time2dollars.git"
  },
  "keywords": [
    "cyclejs",
    "time-to-dollars"
  ],
  "author": "Karl Ranna",
  "license": "MIT",
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^9.2.2",
    "@cycle/isolate": "^1.2.0",
    "material-design-lite": "^1.1.3",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "karma": "^0.13.22",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "style-loader": "^0.13.1",
    "surge": "^0.18.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
